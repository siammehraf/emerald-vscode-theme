{
  "name": "Emerald Color Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#161A0E",
    "editor.foreground": "#EAEFD3",
    "editorGroupHeader.tabsBackground": "#1b2713",
    "editorGroupHeader.emptyBackground": "#FFFFFF",
    "sideBar.background": "#161A0E",
    "activityBar.background": "#161A0E",
    "statusBar.background": "#161A0E",
    "titleBar.activeBackground": "#161A0E",
    "terminal.background": "#1b2713",
    "terminal.foreground": "#EAEFD3",
    "terminalCursor.background": "#EAEFD3",
    "terminalCursor.foreground": "#EAEFD3",
    "tab.activeBackground": "#161A0E",
    "tab.inactiveBackground": "#161A0E",
    "tab.border": "#161A0E",
    "tab.activeBorderTop": "#EAEFD3",
    "tab.activeBorder": "#EAEFD3",
    "tab.hoverBackground": "#1F2315",
    "sideBarTitle.foreground": "#EAEFD3",
    "sideBarSectionHeader.background": "#161A0E",
    "list.activeSelectionBackground": "#2A2E16",
    "list.hoverBackground": "#1F2315",
    "list.inactiveSelectionBackground": "#232812"
  },
  "tokenColors": [
    {
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "invalid.deprecated",
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FFD700" // vibrant yellow
      }
    },
    {
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "entity.name.function",
        "punctuation.definition.interpolation.end.bracket.curly.scss",
        "punctuation.definition.interpolation.begin.bracket.curly.scss"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "punctuation.definition.interpolation.end.bracket.curly.scss",
        "punctuation.definition.interpolation.begin.bracket.curly.scss"
      ],
      "settings": {
        "foreground": "#FF6EC7" // bright magenta
      }
    },
    {
      "scope": ["constant.other.php"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF3C38"
      }
    },
    {
      "scope": ["Keyword", "Storage"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation.definition.tag",
        "punctuation",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "entity.name.tag.reference.scss"
      ],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "scope": ["punctuation.definition.tag.begin.html", "punctuation.definition.tag.end.html"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "scope": [
        "keyword.control.at-rule",
        "punctuation.terminator.rule.css",
        "keyword.control.operator",
        "punctuation.definition.entity.css",
        "keyword.operator.assignment.js",
        "punctuation.separator.comma.js",
        "punctuation.definition.string.begin.js",
        "punctuation.definition.string.end.js",
        "punctuation.definition.binding-pattern.object.js"
      ],
      "settings": {
        "foreground": "#EAEFD3"
      }
    },
    {
      "scope": ["punctuation.definition", "string.quoted.single.scss"],
      "settings": {
        "foreground": "#A6A6A6"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF4500"
      }
    },
    {
      "scope": [
        "comment.line.scss",
        "comment.line.double-slash.js",
        "punctuation.definition.comment.scss",
        "punctuation.definition.comment.js",
        "punctuation.definition.comment.html",
        "comment.block.html"
      ],
      "settings": {
        "foreground": "#6B8E23",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["text.html.derivative"],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#FF3C38"
      }
    },
    {
      "scope": ["entity.name.tag.html"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["variable.parameter.js", "variable.parameter.keyframe-list.css"],
      "settings": {
        "foreground": "#00FFAF"
      }
    },
    {
      "scope": [
        "variable.other.object.js",
        "storage.type.js",
        "string.other.link.title.markdown",
        "variable.other.readwrite.js",
        "variable.other.property.js",
        "entity.name.type.module.js"
      ],
      "settings": {
        "foreground": "#00E0FF"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted.double.html",
        "variable.other.constant.js",
        "variable.other.object.property.js",
        "string.quoted.double.json",
        "markup.fenced_code.block.markdown",
        "markup.inline.raw.string.markdown",
        "variable.function",
        "keyword.other.special-method",
        "entity.name.function",
        "markup.bold",
        "markup.bold.markdown",
        "markup.italic.markdown",
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor",
        "entity.name.tag.other.html",
        "entity.name.tag.block.any.html",
        "source.sass keyword.control"
      ],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "scope": ["punctuation.definition.string.begin.html", "punctuation.definition.string.end.html"],
      "settings": {
        "foreground": "#C4CBFF"
      }
    },
    {
      "scope": ["support.other.variable", "string.other.link", "markup.table"],
      "settings": {
        "foreground": "#EAEFD3"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "scope": ["variable.parameter.function.language.special", "variable.parameter"],
      "settings": {
        "foreground": "#00FFAF"
      }
    },
    {
      "scope": [
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "keyword.other.unit",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#FFB400"
      }
    },
    {
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#00E0FF"
      }
    },
    {
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#5DA9E9"
      }
    },
    {
      "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
      "settings": {
        "foreground": "#00FFAF"
      }
    },
    {
      "scope": [
        "entity.name.method.js",
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "support.function"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.pseudo-class",
        "entity.name.tag.css",
        "keyword.control.conditional.js",
        "support.constant",
        "entity.other.attribute-name.html",
        "string.quoted.single.js",
        "storage.type.function.js",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "scope": ["source.scss keyword.control"],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF3C38"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#FF00F2"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#C4CBFF"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#C4CBFF"
      }
    },
    {
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "foreground": "#EAEFD3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#EAEFD3"
      }
    },
    {
      "scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
      "settings": {
        "foreground": "#EAEFD3"
      }
    },
    {
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"],
      "settings": {
        "foreground": "#5966cc50"
      }
    },
    {
      "scope": ["text.html.markdown meta.dummy.line-break"],
      "settings": {}
    },
    {
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strike"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#5966cc50"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["punctuation.definition.raw.markdown", "punctuation.definition.markdown"],
      "settings": {
        "foreground": "#C4CBFF"
      }
    },
    {
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#5966cc"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#BBBBBB",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#5966cc"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#FFB400"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#FF3C38"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#C4CBFF"
      }
    }
  ]
}
